{
  "name": "@jairemix/capacitor-segment",
  "version": "0.0.1",
  "description": "capacitor wrapper plugin for Segment SDK",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rm -rf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "yarn build",
    "release": "yarn build && yarn publish --access public"
  },
  "author": "Jeremy Li",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "latest"
  },
  "devDependencies": {
    "typescript": "^3.2.4",
    "@capacitor/ios": "latest",
    "@capacitor/android": "latest"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorPluginSegment.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jairemix/capacitor-segment"
  },
  "bugs": {
    "url": "https://github.com/jairemix/capacitor-segment/issues"
  }
}
